{
    "git.confirmSync": false,
    "git.autofetch": true,
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.fontFamily": "'Cascadia Code', 'Noto Sans SC'",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.fontWeightBold": "bold",
    "terminal.integrated.cursorStyle": "block",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.smoothScrolling": true,
    "editor.fontFamily": "'Google Sans Code', 'Cascadia Code', 'Noto Sans SC'",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.fontWeight": 500,
    "editor.scrollbar.horizontalScrollbarSize": 6,
    "editor.scrollbar.verticalScrollbarSize": 6,
    "editor.smoothScrolling": true,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.lineNumbers": "relative",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.guides.bracketPairs": "active",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1500,
    "files.encoding": "utf8",
    "files.eol": "\n",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.compactFolders": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.maxComputationTime": 0,
    "diffEditor.renderSideBySide": true,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "window.autoDetectColorScheme": true,
    "workbench.startupEditor": "none",
    "workbench.preferredLightColorTheme": "Default Light Modern",
    "workbench.preferredDarkColorTheme": "Default Dark Modern",
    "workbench.list.smoothScrolling": true,
	"workbench.reduceMotion": "off",
    "workbench.iconTheme": "material-icon-theme",
    "vim.foldfix": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.duration": 300,
    "vim.highlightedyank.color": "rgba(85, 107, 47, 0.8)",
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.leader": "<space>",
    "vim.easymotion": true,
    "vim.surround": true,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": [ "#8FBCBB", "#434C5E" ],
    "vim.statusBarColors.insert": "#BF616A",
    "vim.statusBarColors.visual": "#B48EAD",
    "vim.statusBarColors.visualline": "#B48EAD",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": "#007ACC",
    "vim.statusBarColors.searchinprogressmode": "#007ACC",
    "vim.statusBarColors.easymotionmode": "#007ACC",
    "vim.statusBarColors.easymotioninputmode": "#007ACC",
    "vim.statusBarColors.surroundinputmode": "#007ACC",
    "workbench.colorCustomizations": {
        "statusBar.background": "#8FBCBB",
        "statusBar.noFolderBackground": "#8FBCBB",
        "statusBar.debuggingBackground": "#8FBCBB",
        "statusBar.foreground": "#434C5E",
        "statusBar.debuggingForeground": "#434C5E"
    },
    "vim.handleKeys": {
        "<C-d>": false,
        "<C-s>": false,
        "<C-z>": false,
        "<C-f>": false,
        "<C-b>": false,
        "<C-p>": false
    },
    "vim.normalModeKeyBindings": [
        { "before": [ "H" ], "after": [ "^" ] },
        { "before": [ "L" ], "after": [ "$" ] },
        { "before": [ "J" ], "after": [ "5", "j" ] },
        { "before": [ "K" ], "after": [ "5", "k" ] },
        { "before": [ "<C-h>" ], "after": [ "<C-w>", "h" ] },
        { "before": [ "<C-l>" ], "after": [ "<C-w>", "l" ] },
        { "before": [ "<C-j>" ], "after": [ "g", "T" ] },
        { "before": [ "<C-k>" ], "after": [ "g", "t" ] },
        { "before": [ "g", "j" ], "commands": [ "workbench.action.navigateBack" ] },
        { "before": [ "g", "k" ], "commands": [ "workbench.action.navigateForward" ] }
    ],
    "vim.visualModeKeyBindings": [
        { "before": ["L"], "after": ["$"] },
        { "before": ["H"], "after": ["^"] },
        { "before": [ "J" ], "after": [ "5", "j" ] },
        { "before": [ "K" ], "after": [ "5", "k" ] }
    ],
    "vim.operatorPendingModeKeyBindings": [
        { "before": ["L"], "after": ["$"] },
        { "before": ["H"], "after": ["^"] },
        { "before": [ "J" ], "after": [ "5", "j" ] },
        { "before": [ "K" ], "after": [ "5", "k" ] }
    ],
    "dev.containers.dockerPath": "podman",
	"dev.containers.dockerComposePath": "podman-compose",
    "remote.autoForwardPortsSource": "hybrid"
}