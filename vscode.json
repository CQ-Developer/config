{
    "remote.autoForwardPortsSource": "hybrid",
    "chat.disableAIFeatures": true,
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.fontFamily": "'Cascadia Code', 'Noto Sans SC'",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.fontWeightBold": "bold",
    "terminal.integrated.cursorStyle": "block",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.smoothScrolling": true,
    "editor.fontFamily": "'Google Sans Code', 'Cascadia Code', 'Noto Sans SC'",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.fontWeight": 500,
    "editor.scrollbar.horizontalScrollbarSize": 6,
    "editor.scrollbar.verticalScrollbarSize": 6,
    "editor.smoothScrolling": true,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.lineNumbers": "relative",
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.guides.bracketPairs": "active",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 3000,
    "files.encoding": "utf8",
    "files.eol": "\n",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.compactFolders": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.maxComputationTime": 0,
    "diffEditor.renderSideBySide": true,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "window.autoDetectColorScheme": true,
    "workbench.startupEditor": "none",
    "workbench.preferredLightColorTheme": "Monokai Pro Light (Filter Sun)",
    "workbench.preferredDarkColorTheme": "Monokai Pro (Filter Spectrum)",
    "workbench.list.smoothScrolling": true,
	"workbench.reduceMotion": "off",
    "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
    "workbench.editor.empty.hint": "hidden",
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#8b4513",
        "statusBar.background": "#222222",
        "statusBar.noFolderBackground": "#222222",
        "statusBar.debuggingBackground": "#222222",
        "statusBar.foreground": "#ffffff",
        "statusBar.debuggingForeground": "#ffffff"
    },
    "vim.foldfix": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.duration": 300,
    "vim.highlightedyank.color": "rgba(85, 107, 47, 0.8)",
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.leader": "<space>",
    "vim.easymotion": true,
    "vim.surround": true,
    "vim.statusBarColorControl": true,
	"vim.statusBarColors.normal": [ "#222222", "#ffffff" ],
	"vim.statusBarColors.insert": [ "#483d8b", "#ffffff" ],
	"vim.statusBarColors.visual": [ "#5f00af", "#ffffff" ],
	"vim.statusBarColors.visualblock": [ "#86592d", "#ffffff" ],
	"vim.statusBarColors.visualline": [ "#005f87", "#ffffff" ],
	"vim.statusBarColors.replace": [ "#808000", "#ffffff" ],
	"vim.statusBarColors.commandlineinprogress": [ "#4169e1", "#ffffff" ],
	"vim.statusBarColors.searchinprogressmode": [ "#4169e1", "#ffffff" ],
	"vim.statusBarColors.easymotionmode": [ "#4169e1", "#ffffff" ],
	"vim.statusBarColors.easymotioninputmode": [ "#4169e1", "#ffffff" ],
	"vim.statusBarColors.surroundinputmode": [ "#4169e1", "#ffffff" ],
    "vim.handleKeys": {
        "<C-d>": false,
        "<C-s>": false,
        "<C-z>": false,
        "<C-f>": false,
        "<C-b>": false,
        "<C-p>": false
    },
    "vim.normalModeKeyBindings": [
        { "before": [ "H" ], "after": [ "^" ] },
        { "before": [ "L" ], "after": [ "$" ] },
        { "before": [ "J" ], "after": [ "5", "j" ] },
        { "before": [ "K" ], "after": [ "5", "k" ] },
        { "before": [ "<C-h>" ], "after": [ "<C-w>", "h" ] },
        { "before": [ "<C-l>" ], "after": [ "<C-w>", "l" ] },
        { "before": [ "<C-j>" ], "after": [ "g", "T" ] },
        { "before": [ "<C-k>" ], "after": [ "g", "t" ] },
        { "before": [ "g", "j" ], "commands": [ "workbench.action.navigateBack" ] },
        { "before": [ "g", "k" ], "commands": [ "workbench.action.navigateForward" ] },
        { "before": [ "Q" ], "commands": [ "editor.action.formatDocument" ] }
    ],
    "vim.visualModeKeyBindings": [
        { "before": ["L"], "after": ["$"] },
        { "before": ["H"], "after": ["^"] },
        { "before": [ "J" ], "after": [ "5", "j" ] },
        { "before": [ "K" ], "after": [ "5", "k" ] }
    ],
    "vim.operatorPendingModeKeyBindings": [
        { "before": ["L"], "after": ["$"] },
        { "before": ["H"], "after": ["^"] },
        { "before": [ "J" ], "after": [ "5", "j" ] },
        { "before": [ "K" ], "after": [ "5", "k" ] }
    ]
}